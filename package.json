{
  "name": "tree-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf ./build && rimraf ./buildScripts/build",
    "build-build": "npm run clean && tsc -p ./buildScripts/tsconfig.json && cd ./buildScripts && go build ./gzipper.go && cd ../",
    "run-build-dev": "env-cmd -e dev node ./buildScripts/build/build.js",
    "run-build-prod": "env-cmd -e prod node ./buildScripts/build/build.js",
    "run-build-client-dev": "env-cmd -e dev node ./buildScripts/build/buildClientScript.js",
    "run-build-server-dev": "env-cmd -e dev node ./buildScripts/build/buildServerScript.js",
    "build-dev": "npm run build-build && npm run run-build-dev",
    "build-prod": "npm run build-build && npm run run-build-prod",
    "start-server-dev": "env-cmd -e dev node ./build/server/out.js",
    "watch-client-dev": "env-cmd -e dev nodemon --watch ./src/client --watch ./src/common --ext \"ts,tsx,json\" --exec \"npm run run-build-client-dev\"",
    "watch-server-dev": "env-cmd -e dev nodemon --watch ./src/server --watch ./src/common --ext \"ts,tsx,json\" --exec \"npm run run-build-server-dev && npm run start-server-dev\"",
    "start": "npm run build-build && concurrently -n \"ui ,api\" -c \"cyan,blue\" \"npm run watch-client-dev\" \"npm run watch-server-dev\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.1",
    "concurrently": "^6.5.1",
    "connect-livereload": "^0.6.1",
    "connected-react-router": "^6.9.2",
    "cors": "^2.8.5",
    "esbuild-plugin-sass": "^0.6.0",
    "esbuild-sass-plugin": "^2.0.0",
    "express": "^4.17.2",
    "history": "^4.10.1",
    "jsdom": "^19.0.0",
    "livereload": "^0.9.3",
    "nodemon": "^2.0.15",
    "react-router-dom": "^6.2.1",
    "redux-thunk": "^2.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/connect-livereload": "^0.6.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsdom": "^16.2.14",
    "@types/livereload": "^0.9.1",
    "@types/node": "^17.0.5",
    "@types/react-dom": "^17.0.11",
    "env-cmd": "^10.1.0",
    "esbuild": "^0.13.15",
    "pretty-bytes": "^5.6.0",
    "ts-node": "^10.4.0"
  }
}
